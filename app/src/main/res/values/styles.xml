<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ProgressBarTheme">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateTintMode">src_atop</item>
        <item name="android:indeterminateTint">@color/colorPrimary</item>
    </style>

    <style name="textAppearanceLabel" parent="android:Widget">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#232323</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontWeight">700</item>
    </style>

    <style name="textAppearanceContent" parent="android:Widget">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#AA000000</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontWeight">700</item>
    </style>

</resources>